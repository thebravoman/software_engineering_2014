class Student
    @@firstname = ""
    @@lastname = ""
    @@teamname = ""
    @@results = []
    def initialize(first_name, last_name, team_name)
        @@firstname = first_name
        @@lastname = last_name
        @@teamname = team_name
    end

    def first
        return @@firstname
    end
    
    def last
        return @@firstname
    end
    
    def team
        return @@teamname
    end
    
    def result_in results
        @@results = results
    end
    
    def result_out
        return @@results
    end
end
